{
  "name": "quiz",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "ng": "cd ./angular-project && ng",
    "start:ng": "cd ./angular-project && ng serve",
    "start:server": "node index.js",
    "start:server:watch": "nodemon index.js",
    "build": "cd ./angular-project && ng build",
    "watch": "cd ./angular-project && ng build --watch --configuration development",
    "test": "cd ./angular-project && ng test",
    "dev": "concurrently \"npm run start:ng\" \"npm run start:server\"",
    "mon": "concurrently \"npm run start:ng\" \"npm run start:server:watch\""
  },
